{
    "name": "parsilver/thailand-provinces-php",
    "description": "Thailand address database",
    "keywords": ["province", "thailand", "amphure", "address"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "PA",
            "role": "DevOps"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
